<div class="col-md-9">
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Created</th>
            </tr>
        </thead>
        <tbody>
            {% for student in students %}  
            <tr>
                <th scope="row">{{ student.id }}</th>
                <td>{{ student.name }}</td>
                <td>{{ student.email }}</td>
                <td>{{ student.created | date("F j, Y") }}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>